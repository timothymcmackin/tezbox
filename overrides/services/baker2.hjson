{
    "restart_max_retries": 0,
    "user": "tezos",
    "stop_timeout": 300,
    "executable": "octez-baker-PsParisC",
    "restart_delay": 20,
    "environment": {
        "HOME": "/tezbox/data",
        "TEZOS_LOG": "* -> info"
    },
    "restart": "always",
    "args": [
    "run",
    "with",
    "local",
    "node",
    "/tezbox/data/.tezos-node2",
    "baker2",
    "--votefile",
    "/tezbox/context/protocols/PsParisC/vote-file.json",
    "--dal-node",
    "http://127.0.0.1:10733"
]
}